    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.0.14",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "copyfiles": "^2.4.1",
    "eslint": "9.31.0",
    "eslint-config-next": "15.4.1",
